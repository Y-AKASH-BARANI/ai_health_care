# ===========================
# Client .gitignore (Next.js)
# ===========================

# ---------- Dependencies ----------
/node_modules
/.pnp
.pnp.*
.yarn/*
!.yarn/patches
!.yarn/plugins
!.yarn/releases
!.yarn/versions

# ---------- Next.js ----------
/.next/
/out/

# ---------- Production build ----------
/build
/dist

# ---------- Testing ----------
/coverage

# ---------- Environment variables ----------
.env
.env.*
!.env.example
!.env.local.example

# ---------- Vercel ----------
.vercel

# ---------- TypeScript ----------
*.tsbuildinfo
next-env.d.ts

# ---------- Tailwind ----------
.tailwindcss/

# ---------- Debug / Logs ----------
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.pnpm-debug.log*

# ---------- OS files ----------
.DS_Store
Thumbs.db

# ---------- IDE / Editor ----------
.vscode/
.idea/
*.swp
*.swo

# ---------- PWA (generated by next-pwa) ----------
/public/sw.js
/public/sw.js.map
/public/workbox-*.js
/public/workbox-*.js.map
/public/swe-worker-*.js

# ---------- Temp / Claude artifacts ----------
tmpclaude-*
